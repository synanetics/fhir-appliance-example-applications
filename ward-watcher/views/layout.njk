<!DOCTYPE html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">

    <title>{% block pageTitle %}St. Mungos Ward Watcher{% endblock %}</title>

    
    <link href="https://fonts.gstatic.com/" rel="preconnect" crossorigin> 
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@500;700&display=swap" rel="stylesheet" crossorigin>
    
    {% block headCSS %}
    <link href="/ward-watcher/main.css" rel="stylesheet">
    {% endblock %}
    <script src="/ward-watcher/nhsuk.js" defer></script>

    <script src="https://kit.fontawesome.com/a56fa1151a.js" crossorigin="anonymous"></script>
    
    {% block headIcons %}
    <link rel="shortcut icon" href="{{ baseUrl }}assets/favicons/favicon.ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="{{ baseUrl }}assets/favicons/apple-touch-icon-180x180.png">
    <link rel="mask-icon" href="{{ baseUrl }}assets/favicon.svg" color="#005eb8">
    <link rel="icon" sizes="192x192" href="{{ baseUrl }}assets/favicon-192x192.png">
    <meta name="msapplication-TileImage" content="{{ baseUrl }}assets/mediumtile-144x144.png">
    <meta name="msapplication-TileColor" content="#005eb8">
    <meta name="msapplication-square70x70logo" content="{{ baseUrl }}assets/smalltile-70x70.png">
    <meta name="msapplication-square150x150logo" content="{{ baseUrl }}assets/mediumtile-150x150.png">
    <meta name="msapplication-wide310x150logo" content="{{ baseUrl }}assets/favicons/widetile-310x150.png">
    <meta name="msapplication-square310x310logo" content="{{ baseUrl }}assets/favicons/largetile-310x310.png">
    {% endblock %}
  </head>

  <body class="{{ bodyClasses }}">
    <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>
        {% block bodyStart %}
        {% endblock %} 

        {% block skipLink %} 
          {{ skipLink({ href: '#maincontent', text: 'Skip to main content' }) }} 
        {% endblock %} 

        {% block header %} 
          {{ header({}) }} 
        {% endblock %} 

        {% block beforeContent %}
        {% endblock %} 
        
        {% block main %}
        <div class="nhsuk-width-container {{ containerClasses }}">
            <main class="nhsuk-main-wrapper {{ mainClasses }}" id="maincontent" role="main">
              {% block content %}
              {% endblock %}
            </main>
        </div>
        {% endblock %} 
        
        {% block footer %} 
        {{ footer({
            "links": [
              {
                "URL": "https://yhcr.org/",
                "label": "YHCR"
              },
              {
                "URL": "https://github.com/yorkshire-and-humber-care-record/fhir-appliance",
                "label": "FHIR Appliance"
              },
              {
                "URL": "https://github.com/synanetics/fhir-appliance-example-applications",
                "label": "Example Applications"
              },
              {
                "URL": "https://github.com/nhsuk/nhsuk-frontend",
                "label": "NHS UK Frontend"
              }
            ]
          })}}
        {% endblock %} 

        {% block bodyEnd %}
        {% endblock %}
  </body>
</html>
